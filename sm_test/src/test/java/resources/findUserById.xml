<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="findUserById">
    <test name="根据id修改用户">
        <parameter name="id" value="10"></parameter>
        <classes>
            <class name="apis.FindUserById"></class>
        </classes>
    </test>
    <!-- 设置监听器 ,运行testng.xml在test-ouput里面生成html格式的报告，邮件用浏览器打开即可看结果-->
    <listeners>
        <listener class-name="config.ExtentTestNGIReporterListener"></listener>
    </listeners>
</suite>